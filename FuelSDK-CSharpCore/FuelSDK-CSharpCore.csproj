<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <RootNamespace>FuelSDK_CSharpCore</RootNamespace>
        <Nullable>enable</Nullable>
        <PackageId>Twiddy-FuelSDK-CSharpCore</PackageId>
    </PropertyGroup>

    <ItemGroup>
      <None Update="App.config.transform">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </None>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="JWT" Version="10.0.2" />
      <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
      <PackageReference Include="System.Configuration.ConfigurationManager" Version="7.0.0" />
      <PackageReference Include="System.ServiceModel.Http" Version="4.10.2" />
      <PackageReference Include="System.ServiceModel.Primitives" Version="4.10.2" />
      <PackageReference Include="System.ServiceModel.Security" Version="4.10.2" />
    </ItemGroup>

    <ItemGroup>
      <Reference Include="Microsoft.CSharp" />
    </ItemGroup>

</Project>
